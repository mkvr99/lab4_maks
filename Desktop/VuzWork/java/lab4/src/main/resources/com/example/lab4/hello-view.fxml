<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="331.0" prefWidth="307.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab4.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
   <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
      <children>
          <Button onAction="#onHelloButtonClick" text="→" />
            <Button mnemonicParsing="false" onAction="#onButton2" text="←" />
         <Button mnemonicParsing="false" onAction="#onStart" text="start" />
         <Button layoutX="83.0" layoutY="10.0" mnemonicParsing="false" onAction="#onPause" text="pause" />
         <Button layoutX="125.0" layoutY="10.0" mnemonicParsing="false" onAction="#onStop" text="stop" />
      </children></HBox>
   <TextField fx:id="textField" />
   <Button mnemonicParsing="false" onAction="#onSetTime" text="Установить время задержки" />
</VBox>
